.navbar_content{
  padding: 0 30rpx;
  height: 100%;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1000;
}
.navbar_content .title{
  width: 100%;
  font-family: MicrosoftYaHei;
	font-size: 30rpx;
	letter-spacing: 2rpx;
	color: #000000;
  display: flex;
  align-items: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}