<!--pages/customerService/customerService.wxml-->
<navnavbar></navnavbar>
<nav-back inner-text="我的客服"></nav-back>
<view class="container"  style="height: calc(100% - {{customNavBarHeight}}px - 82rpx)">
    <view class="scancode_box">
      <view>
        <image src="{{baseUrl + kfInfo.kfwx}}" mode="widthFix"></image>
        <view class="tip">
          <view>添加客服微信二维码</view>
          <view>我们在线为您解答</view>
        </view>
      </view>
    </view>
    <view class="phone_box">
      <text catchtap="callPone" class="phone_num" user-select='{{true}}'>{{kfInfo.kfdh}}</text>
      <view class="tip">您也可以拨打客服热线</view>
    </view>
</view>

