.caontent{
  width: 100%;
}
.photo{
  width: 100%;
  height: 300rpx;
  position: relative;
  background-color: #939597;
}
.back{
  width: 100%;
  height: 100%;
}
.back>image{
  width: 100%;
  height: 100%;
}
.avatar{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  background-color: #939597;
  border: 1rpx solid #939597;
  overflow: hidden;
  position: absolute;
  bottom: -80rpx;
  left: 20rpx;
}
.avatar image{
  width: 100%;
  height: 100%;
}
.form{
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx ;
  padding-top: 90rpx;
  font-size: larger;
}
.label{
  width: 200rpx;
}
.inputOuter{
  padding: 6rpx 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.myplaceholder{
  color: #939597;
}
.inputOuter>input{
  width: 450rpx;
  padding: 10rpx 6rpx;
 border-bottom: 1rpx solid  #939597;
}
picker{
  width: 450rpx;
  padding: 10rpx 6rpx;
  border-bottom: 1rpx solid  #939597;
}
.provinceOuter{
  display: flex;
  padding: 6rpx 0;
  align-items: flex-start;
}
.input{
  width: 450rpx;
  display: flex;
  justify-content: start;
  justify-items: center;
  flex-wrap: wrap;
}
.input>input{
  text-align: center;
  height: 45rpx;
  line-height: 45rpx;
  padding-bottom: 10rpx;
  margin-bottom: 10rpx;
  border-bottom: 1rpx solid  #939597;
}
.input :nth-child(1){
  width: 100rpx;
}
.input :nth-child(3){
  width: 200rpx;
}
.input :nth-child(5){
  width: 400rpx;
}
.input>text{
  height: 45rpx;
  line-height: 45rpx;
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid  #939597;
}
.placeholder{
  width: 100%;
  text-align: center !important;
}
radio-group{
  width: 450rpx;
  padding: 10rpx 6rpx;
  margin-bottom: 10rpx;
}
.radioOuter{
  padding: 6rpx 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.pickerOuter{
  padding: 6rpx 0;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
label{
  padding-right: 20rpx;
}
button{
  width: 100% !important;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  font-weight: normal !important;
}
button[type='primary'] {
  background-color: #F5DF4D !important;
  color: #939597;
}
button[type='default'] {
  background-color: #d81e06 !important;
  color: #939597 !important;
}
.button-sp-area{
  padding: 200rpx 0;
}
.textareaOut{
  margin-top: 20rpx;
  padding: 6rpx 0;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}
.textareaOut >textarea{
  border: 1rpx solid #939597;
  padding: 15rpx ;
  border-radius: 8rpx;
  width: 450rpx;
  padding: 10rpx 6rpx;
}