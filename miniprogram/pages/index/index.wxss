
.content{
  height: 100%;
  position: relative;
  width: 100%;
  overflow: hidden;
}
.backgroud{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.mask{
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.123);
}
.formSubmitOut{
  width: 80%;
}
input{
  box-sizing: border-box;
  height: 100rpx;
  width: 100%;
  border-radius: 50rpx !important;
  display: block;
  font-size: 32rpx;
  line-height: 50rpx;
  margin-bottom: 60rpx;
  padding:20rpx 50rpx;
  color: #000;
  background-color: rgba(255, 255, 255, 0.281);
}
.input{
  color: #000;
}
button{
  height: 100rpx;
  line-height: 100rpx !important;
  padding: 0 !important;
  width: 100% !important;
  border: 1rpx solid #ffdf2a;
  letter-spacing: 3px;
  border-radius: 50rpx !important;
  background-color: #F5DF4D;
  color: #000;
  font-size: 15px;
  animation: ani 0.8s ease  infinite  alternate;
}
@keyframes ani{
 to{
  -webkit-box-shadow: 0px 5px 10px #cf9205bd;
  box-shadow: 0px 5px 10px #cf9205bd;
 }
}
.submit_hover{
  background-color: yellow;
  color: #000;
  border-color: yellow;
}

.links{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  padding:40rpx 0;
  color: #fff;
}
.links navigator{
  border-bottom: 1px solid #fff;
}
.hover{
  color: #F5DF4D;
  border-color: #F5DF4D;
}
.links view{
  padding: 0 10rpx;
}
.copyRight{
  padding: 20rpx 0;
  width: 100%;
  position: absolute;
  bottom: 0;
  text-align: center;
  color: #90939991;
  font-size: 12px;
}