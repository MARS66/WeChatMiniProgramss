
.search{
  height: 300rpx ;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.search>image{
  height: 300rpx ;
  width: 100%;
}
.search>view{
  height: 300rpx ;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search-input{
  display: flex;
  width: 80%;
  justify-content: start;
  border-radius: 35rpx;
  align-items: center;
  height: 70rpx;
  background-color: #939597ea;
}
.container{
  box-sizing: border-box;
  height: 100%;
  width: 100%;
  margin-top: 300rpx;
  overflow-y: auto;
}
.all{
  width: 100%;
}
.search-input>input{
  box-sizing: border-box;
  height: 100%;
  width: 80%;
  color: white;
  padding: 10rpx 20rpx;
}
.search-input>text{
  box-sizing: border-box;
  background-color: #F5DF4D;
  color: #939597;
  border-radius: 35rpx;
  text-align: center;
  height: 100%;
  width: 30%;
  padding: 10rpx;
  padding: 10rpx 20rpx;
}
.placeholder{
  color: rgba(224, 221, 221, 0.993);
}
.photoOuter{
  box-sizing: border-box;
  padding: 10rpx 20rpx;
  width: 100%;
}
.title{
  padding: 10rpx 0;
  font-weight: 600;
  font-size: 34rpx;
  text-align: start;
}
.list{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.smallhoto{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-right: 10rpx;
}
.smallhoto>image{
width: 120rpx;
height: 120rpx;
background-color:#939597;
border-radius: 50%;
}
.smallhoto>view{
  text-align: center;
  font-size: medium;
  border-radius: 8rpx;
  margin-top: 10rpx;
  font-weight: 600;
}
.epmty{
  box-sizing: border-box;
  width: 100%;
  padding: 20rpx 10rpx;
  height: 200rpx;
}
.epmty2{
  color: blue;
}